{
	"name": "PL_REST_TO_RAW_PAGED",
	"properties": {
		"activities": [
			{
				"name": "ForEach1",
				"type": "ForEach",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"items": {
						"value": "[\n  {\n    \"table\": \"customers\",\n    \"endpoint\": \"/customers\"\n  },\n  {\n    \"table\": \"products\",\n    \"endpoint\": \"/products\"\n  },\n  {\n    \"table\": \"orders\",\n    \"endpoint\": \"/orders\"\n  }\n]\n",
						"type": "Expression"
					},
					"isSequential": true,
					"activities": [
						{
							"name": "Copy data",
							"type": "Copy",
							"dependsOn": [],
							"policy": {
								"timeout": "0.12:00:00",
								"retry": 0,
								"retryIntervalInSeconds": 30,
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"source": {
									"type": "RestSource",
									"httpRequestTimeout": "00:01:40",
									"requestInterval": "00.00:00:00.010",
									"requestMethod": "GET"
								},
								"sink": {
									"type": "ParquetSink",
									"storeSettings": {
										"type": "AzureBlobFSWriteSettings"
									},
									"formatSettings": {
										"type": "ParquetWriteSettings"
									}
								},
								"enableStaging": false
							},
							"inputs": [
								{
									"referenceName": "ds_RestResource",
									"type": "DatasetReference",
									"parameters": {
										"relativeUrl": "@item().endpoint"
									}
								}
							],
							"outputs": [
								{
									"referenceName": "ls_Parquet",
									"type": "DatasetReference",
									"parameters": {
										"folderpath": "@concat(   'raw/',   item().table,   '/ingest_date=',   formatDateTime(utcNow(),'yyyy-MM-dd') )"
									}
								}
							]
						}
					]
				}
			}
		],
		"annotations": []
	}
